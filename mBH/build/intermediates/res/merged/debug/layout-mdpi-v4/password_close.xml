<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
   android:visibility="invisible"
   >
    
    <RelativeLayout 
            android:id="@+id/RL_elevator"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="200dp"
            android:background="@drawable/elevator_set"
            >
            
    	    <TextView 
  		        android:id="@+id/TV_passwordSet"
  		         android:layout_width="wrap_content"
          		  android:layout_height="wrap_content"
          		  android:text="@string/passwordSet"
          		  android:textSize="30sp"
          		  android:layout_marginTop="20dp"
          		 android:layout_centerHorizontal="true"
  		        />
    	    
    	    <ImageButton 
    	        android:id="@+id/IB_closePasswordSet"
    	         android:layout_width="wrap_content"
          		  android:layout_height="wrap_content"
          		  android:layout_marginLeft="20dp"
          		  android:layout_marginTop="100dp"
    	         android:background="@drawable/gray_lock"
    	        />
    	    
    	      <TextView 
  		        android:id="@+id/TV_openPasswordSet"
  		         android:layout_width="wrap_content"
          		  android:layout_height="wrap_content"
          		  android:layout_alignTop="@+id/IB_closePasswordSet"
          		  android:layout_toRightOf="@+id/IB_closePasswordSet"
          		  android:layout_marginLeft="30dp"
          		  android:text="@string/closePasswordSet"
          		  android:textSize="30sp"	
  		        />
          </RelativeLayout>
    

</RelativeLayout>
