<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="@drawable/factory_pattern_background">
   
     <include android:id="@+id/FactoryPatternset_elevator" layout="@layout/factorypatternset_elevator" />
       <include android:id="@+id/Password_Close" layout="@layout/password_close" />
        <include android:id="@+id/RL_PasswordOpen" layout="@layout/password_open" />
        
    <RelativeLayout  
        android:id="@+id/RL_factoryPatternSet"
          android:layout_width="match_parent"
   		 android:layout_height="match_parent" 
        >
        
         <RelativeLayout
        android:id="@+id/RL_Keyboard_Layout"
        android:layout_width="310dp"
        android:layout_height="360dp"
       android:layout_marginTop="180dp"
    	android:layout_alignLeft="@+id/totalDistanceTextView"
        android:background="@drawable/keyboard_background" >
            
            <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="300dp"
            android:layout_height="360dp"
            android:layout_alignParentTop="true"
           
            android:orientation="vertical"
            >

            <LinearLayout
                android:id="@+id/LL_One"
                android:layout_width="match_parent"
                android:layout_height="70dp"
               
                android:gravity="left" >

                <Button
                    android:id="@+id/Btn_One"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:layout_weight="1"
                    android:clickable="false"
                    android:background="@drawable/one" />

                <Button
                    android:id="@+id/Btn_Two"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:layout_weight="1"
                    android:clickable="false"
                    android:background="@drawable/two" />

                <Button
                    android:id="@+id/Btn_Three"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:layout_weight="1"
                    android:clickable="false"
                    android:background="@drawable/three" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LL_Two"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                 >

                <Button
                    android:id="@+id/Btn_Four"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:clickable="false"
                     android:layout_weight="1"
                    android:background="@drawable/four" />

                <Button
                    android:id="@+id/Btn_Five"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:clickable="false"
                     android:layout_weight="1"
                    android:background="@drawable/five" />

                <Button
                    android:id="@+id/Btn_Six"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:clickable="false"
                     android:layout_weight="1"
                    android:background="@drawable/six" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LL_Three"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                >

                <Button
                    android:id="@+id/Btn_Seven"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:clickable="false"
                     android:layout_weight="1"
                    android:background="@drawable/seven" />

                <Button
                    android:id="@+id/Btn_Eight"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:clickable="false"
                     android:layout_weight="1"
                    android:background="@drawable/eight" />

                <Button
                    android:id="@+id/Btn_Nine"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:clickable="false"
                     android:layout_weight="1"
                    android:background="@drawable/nine" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LL_Four"
                android:layout_width="match_parent"
                android:layout_height="70dp"
               android:layout_marginLeft="0dp" >

                <Button
                    android:id="@+id/Btn_Zero"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:clickable="false"
                    android:layout_weight="1"
                    android:background="@drawable/zero" />

                <Button
                    android:id="@+id/Btn_Point"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:clickable="false"
                    android:layout_weight="1"
                    android:background="@drawable/point" />

                <Button
                    android:id="@+id/Btn_Delete"
                    android:layout_width="wrap_content"
                    android:layout_height="70dp"
                    android:clickable="false"
                    android:layout_weight="1"
                    android:background="@drawable/del" />
            </LinearLayout>
            
            <LinearLayout
                android:id="@+id/LL_Enter"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">
            
        <Button
            android:id="@+id/Btn_Enter"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="false"
            android:background="@drawable/enter" />
        
        </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
    
         <TextView 
             android:id="@+id/TV_addOil"
             android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/RL_Keyboard_Layout"
            android:layout_marginTop="25dp"
           android:layout_marginLeft="480dp"
            android:clickable="true"
            android:text="@string/addOil"
            android:textSize="30sp"
            android:gravity="center"
            android:background="@drawable/add_oil"
             />
        <LinearLayout 
            android:id="@+id/LL_showDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_alignLeft="@+id/totalTimeTextView"
            android:layout_marginTop="200dp"
            android:background="@drawable/show_date"
            >

            <RelativeLayout
                android:id="@+id/RL_SIENG"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
             android:layout_weight="1"
             android:clickable="true"
             android:background="@drawable/small_background"
             >

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/SIENG" 
                    android:layout_weight="1"
                    android:textSize="30sp"/>
                
                <TextView 
                     android:id="@+id/TV_SIENG"
                     android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView1"
                 android:layout_marginLeft="50dp"
           
                    android:gravity="center"
                    android:textSize="30sp"
                    />
           
            </RelativeLayout>
            
              <RelativeLayout
                   android:id="@+id/RL_minSpeed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
             android:layout_weight="1"
              android:clickable="true"
                android:background="@drawable/small_background">
                
                     <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/minSpeed" 
                    android:textSize="30sp"/>
                     
                     <TextView 
                     android:id="@+id/TV_minSpeed"
                     android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView2"
                 android:layout_marginLeft="80dp"
             
                    android:gravity="center"
                    android:textSize="30sp"
                    />
            </RelativeLayout>
            
              <RelativeLayout
                    android:id="@+id/RL_maxSpeed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
             android:layout_weight="1"
              android:clickable="true"
                android:background="@drawable/small_background">
                     
                  <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/maxSpeed" 
                    android:textSize="30sp"/>
                  
                  <TextView 
                     android:id="@+id/TV_maxSpeed"
                     android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView3"
                 android:layout_marginLeft="80dp"
               
                    android:gravity="center"
                    android:textSize="30sp"
                    />
            </RelativeLayout>
            
              <RelativeLayout
                     android:id="@+id/RL_maxIncline"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
             android:layout_weight="1"
              android:clickable="true"
                android:background="@drawable/small_background">
                  
                  <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/maxiNCLINE" 
                    android:textSize="30sp"/>
                  
                  <TextView 
                     android:id="@+id/TV_maxIncline"
                     android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView4"
                 android:layout_marginLeft="80dp"
                  
                    android:gravity="center"
                    android:textSize="30sp"
                    />
            </RelativeLayout>
            
              <RelativeLayout
                   android:id="@+id/RL_speedTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
             android:layout_weight="1"
              android:clickable="true"
                android:background="@drawable/small_background">
                  
                  <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/speedTime" 
                    android:textSize="30sp"/>
                  
                  <TextView 
                     android:id="@+id/TV_speedTime"
                     android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView5"
                 android:layout_marginLeft="80dp"
                  
                    android:gravity="center"
                    android:textSize="30sp"
                    />
            </RelativeLayout>
            
              <RelativeLayout
                   android:id="@+id/RL_whell"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
             android:layout_weight="1"
              android:clickable="true"
                android:background="@drawable/small_background">
                  
                  <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/whell" 
                    android:textSize="30sp"/>
                  
                  <TextView 
                     android:id="@+id/TV_whell"
                     android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView6"
                 android:layout_marginLeft="110dp"
                  
                    android:gravity="center"
                    android:textSize="30sp"
                    />
            </RelativeLayout>
            
              <RelativeLayout
                    android:id="@+id/RL_autoOil"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
             android:layout_weight="1"
              android:clickable="true"
                android:background="@drawable/small_background">
                  
                  <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/autoOil" 
                    android:textSize="30sp"/>
                  
                  <TextView 
                     android:id="@+id/TV_autoOil"
                     android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView7"
                 android:layout_marginLeft="80dp"
                   
                    android:gravity="center"
                    android:textSize="30sp"
                    />
            </RelativeLayout>
            
              <RelativeLayout
                    android:id="@+id/RL_oilTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
             android:layout_weight="1"
              android:clickable="true"
                android:background="@drawable/small_background">
                  
                  <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/oilTime" 
                    android:textSize="30sp"/>
                  
                  <TextView 
                     android:id="@+id/TV_oilTime"
                     android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView8"
                 android:layout_marginLeft="80dp"
                 
                    android:gravity="center"
                    android:textSize="30sp"
                    />
            </RelativeLayout>
            
              <RelativeLayout
                       android:id="@+id/RL_language"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
             android:layout_weight="1"
              android:clickable="true"
                android:background="@drawable/small_background">
                  
                  <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:text="@string/addOilNumber" 
                    android:textSize="30sp"/>
                  
                  <TextView 
                     android:id="@+id/TV_addOilNumber"
                     android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView9"
                 android:layout_marginLeft="20dp"
                   
                    android:gravity="center"
                    android:textSize="30sp"
                    />
            </RelativeLayout>
         
        </LinearLayout>
        <TextView 
            android:id="@+id/totalTimeTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="40dp"
              android:textSize="40sp"
            android:gravity="center"
              android:background="@drawable/factory_pattern_button"
            />
        
       <TextView 
           android:id="@+id/TV_totalTime"
           android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/totalTimeTextView"
            android:layout_marginTop="30dp"
             android:layout_marginLeft="160dp"
             android:text="@string/totalTime"
            android:textSize="30sp"
           />
       
        <TextView 
            android:id="@+id/totalDistanceTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="500dp"
            android:layout_marginTop="40dp"
              android:textSize="40sp"
            android:gravity="center"
              android:background="@drawable/factory_pattern_button"
            />
        
       <TextView 
           android:id="@+id/TV_totalDistance"
           android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/totalTimeTextView"
        	android:layout_alignLeft="@+id/totalDistanceTextView"
        	android:layout_alignRight="@+id/totalDistanceTextView"
        	android:layout_alignTop="@+id/TV_totalTime"
        	android:gravity="center"
             android:text="@string/totalDistance"
            android:textSize="30sp"
           />
       
        <TextView
            android:id="@+id/TV_generalSet"
             android:layout_width="wrap_content"
            android:layout_height="wrap_content"
           android:layout_alignTop="@+id/totalDistanceTextView"
            android:layout_marginLeft="900dp"
            android:layout_marginTop="140dp"
           android:text="@string/generalSet"
            android:textSize="30sp"
            android:gravity="center"
               android:clickable="true"
               android:background="@drawable/set_black"
            />
        
        <TextView
            android:id="@+id/TV_elevator"
             android:layout_width="wrap_content"
            android:layout_height="wrap_content"
          android:layout_below="@+id/TV_generalSet"
          android:layout_marginTop="30dp"
            android:layout_marginLeft="900dp"
           android:text="@string/elevator"
            android:textSize="30sp"
            android:gravity="center"
               android:clickable="true"
               android:background="@drawable/set_black"
            />
        
        <TextView
            android:id="@+id/TV_password"
             android:layout_width="wrap_content"
            android:layout_height="wrap_content"
          android:layout_below="@+id/TV_elevator"
          android:layout_marginTop="30dp"
            android:layout_marginLeft="900dp"
           android:text="@string/password"
            android:textSize="30sp"
            android:gravity="center"
            android:clickable="true"
               android:background="@drawable/set_black"
            />
        
        <TextView
            android:id="@+id/TV_recoverSet"
             android:layout_width="wrap_content"
            android:layout_height="wrap_content"
          android:layout_below="@+id/TV_password"
          android:layout_marginTop="30dp"
            android:layout_marginLeft="900dp"
           android:text="@string/recoverSet"
            android:textSize="20sp"
            android:gravity="center"
               android:clickable="true"
               android:background="@drawable/set_black"
            />
        
         <TextView
            android:id="@+id/TV_exit"
             android:layout_width="wrap_content"
            android:layout_height="wrap_content"
        
          android:layout_alignBottom="@+id/TV_addOil"
       
            android:layout_marginLeft="900dp"
           android:text="@string/exit"
            android:textSize="30sp"
            android:gravity="center"
               android:clickable="true"
               android:background="@drawable/set_black"
            />
       
           <TextView
            android:id="@+id/TV_versions"
             android:layout_width="wrap_content"
            android:layout_height="wrap_content"
         
          android:layout_alignParentBottom="true"
           android:layout_alignLeft="@+id/TV_addOil"
           android:text="@string/versions"
            android:textSize="30sp"
            android:gravity="center"
               android:background="@drawable/set_black"
            />
           
           <TextView 
               android:id="@+id/TV_showVersions"
                android:layout_width="wrap_content"
            	android:layout_height="wrap_content"
            	android:layout_toRightOf="@+id/TV_versions"
            	android:layout_marginLeft="20dp"
            	android:layout_alignBottom="@+id/TV_versions"
            	android:textSize="30dp"
            	android:gravity="center"	
            />
    </RelativeLayout>

</RelativeLayout>
