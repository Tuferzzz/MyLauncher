<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/begin_back_ground">

    <RelativeLayout
        android:id="@+id/RL_Set"
        android:layout_width="900dp"
        android:layout_height="600dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:gravity="center">

        <LinearLayout
            android:id="@+id/ib_set"
            android:layout_width="197dp"
            android:layout_height="246dp"
            android:layout_alignParentTop="true"
            android:layout_marginRight="20dp"
            android:layout_toLeftOf="@+id/ib_mode_now"
            android:background="@drawable/setactivity_set_selector"
            android:orientation="vertical"
            android:focusable="true"
            android:clickable="true">
            <TextView
                android:id="@+id/TV_Set"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/set_per"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="20dp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ib_mode_now"
            android:layout_width="200dp"
            android:layout_height="246dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:background="@drawable/setactivity_nowmode_selector"
            android:orientation="vertical"
            android:focusable="true"
            android:clickable="true">
            <TextView
                android:id="@+id/TV_Mode_Now"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Mode_Now"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="20dp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ib_usb"
            android:layout_width="261dp"
            android:layout_height="118dp"
            android:background="@drawable/setactivity_usb_selector"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/ib_mode_now"
            android:orientation="horizontal"
            android:focusable="true"
            android:clickable="true">
            <TextView
                android:id="@+id/TV_Usb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Usb"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ib_explorer"
            android:layout_width="260dp"
            android:layout_height="118dp"
            android:layout_alignBottom="@+id/ib_mode_now"
            android:layout_alignLeft="@+id/ib_usb"
            android:background="@drawable/setactivity_liulangqi_selector"
            android:orientation="horizontal"
            android:focusable="true"
            android:clickable="true">
            <TextView
                android:id="@+id/TV_Explorer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Explorer"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ib_movie"
            android:layout_width="162dp"
            android:layout_height="203dp"
            android:layout_alignLeft="@+id/ib_set"
            android:layout_below="@+id/ib_set"
            android:layout_marginTop="30dp"
            android:background="@drawable/setactivity_movie_selector"
            android:orientation="vertical"
            android:focusable="true"
            android:clickable="true">
            <TextView
                android:id="@+id/TV_Movie"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Movie"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="160dp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/IB_Av"
            android:layout_width="162dp"
            android:layout_height="203dp"
            android:layout_alignTop="@+id/ib_movie"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/ib_movie"
            android:background="@drawable/setactivity_av_selector"
            android:orientation="vertical"
            android:focusable="true"
            android:clickable="true">
            <TextView
                android:id="@+id/im_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Number_AV"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="160dp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/IB_Tv"
            android:layout_width="162dp"
            android:layout_height="203dp"
            android:layout_alignTop="@+id/ib_movie"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/IB_Av"
            android:background="@drawable/setactivity_tv_selector"
            android:orientation="vertical"
            android:focusable="true"
            android:clickable="true">
            <TextView
                android:id="@+id/TV_Tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Tv"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="160dp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ib_music"
            android:layout_width="162dp"
            android:layout_height="203dp"
            android:layout_alignRight="@+id/ib_usb"
            android:layout_alignTop="@+id/ib_movie"
            android:background="@drawable/setactivity_music_selector"
            android:orientation="vertical"
            android:focusable="true"
            android:clickable="true">
            <TextView
                android:id="@+id/TV_Music"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Music"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="160dp"/>
        </LinearLayout>


    </RelativeLayout>

    <include
        android:id="@+id/Commen_Two"
        layout="@layout/commen" />
</RelativeLayout>